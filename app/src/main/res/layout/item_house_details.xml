<?xml version="1.0" encoding="utf-8"?><!-- 收藏词条 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginTop="15dp"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_house_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fl_pic"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_5"
        android:text="2 Bedroom Apartment"
        android:textColor="@color/color_black"
        android:textSize="@dimen/sp_10" />



    <TextView
        android:id="@+id/tv_bed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@mipmap/fjxqcccc"
        android:layout_below="@+id/tv_house_name"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_5"
        android:text="2"
        android:textSize="10sp"/>

    <TextView
        android:id="@+id/tv_washroom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tv_bed"
        android:drawableLeft="@mipmap/fjxqyyy"
        android:layout_below="@+id/tv_house_name"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_5"
        android:text="2"
        android:textSize="10sp"/>

    <TextView
        android:id="@+id/tv_washmachine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tv_washroom"
        android:drawableLeft="@mipmap/fjxqxxxx"
        android:layout_below="@+id/tv_house_name"
        android:layout_margin="@dimen/dp_5"
        android:text="2"
        android:textSize="10sp"/>

    <TextView
        android:id="@+id/tv_room"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@mipmap/fjxqkkk"
        android:layout_toRightOf="@+id/tv_washmachine"
        android:layout_below="@+id/tv_house_name"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_5"
        android:text="2"
        android:textSize="10sp"/>

    <TextView
        android:id="@+id/tv_kiingroom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@mipmap/fjxqmmmmmm"
        android:layout_below="@+id/tv_bed"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_5"
        android:text="king single 106cm x 203cm"
        android:textSize="10sp"/>

    <TextView
        android:id="@+id/tv_house_note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_kiingroom"
        android:layout_margin="@dimen/dp_5"
        android:text="现有空房，接受预定中"
        android:textColor="@color/color_black"
        android:textSize="@dimen/sp_10" />

    <TextView
        android:id="@+id/tv_house_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/et_username"
        android:layout_alignStart="@+id/et_username"
        android:layout_below="@+id/tv_house_name"
        android:text="$299/周起"
        android:textColor="@color/color_black"
        android:textSize="@dimen/sp_10" />

    <Button
        android:id="@+id/et_username"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tv_kiingroom"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:background="@drawable/button_bg"
        android:text="查看详情"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_8" />


</RelativeLayout>